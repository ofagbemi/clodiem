<script>
  function setlinks() {
	$('a').click( function(e) {
	  e.preventDefault();
	  var h = $(this).attr('href');
	  if(h == '#') h = null;
	  if(h) location.href = h;
	});
  }
  function setlinks_exp() {
	$('a').click( function(e) {
	  e.preventDefault();
	  var h = $(this).attr('href');
	  if(h == '#') h = null;
	  // if(h) location.href = h;
	  if(h) {
	    $('#window').attr('src', h);
	  }
	});
  }
</script>